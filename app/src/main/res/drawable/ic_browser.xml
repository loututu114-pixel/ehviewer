            <path
                android:fillColor="#E0E0E0"
                android:pathData="M7,8H15V10H7V8Z" />
            <!-- 王冠图标 -->
            <path
                android:fillColor="#FFD700"
                android:pathData="M18,4L20,8L22,6L20,10L18,8L16,10L18,6L16,8L18,4Z" />
