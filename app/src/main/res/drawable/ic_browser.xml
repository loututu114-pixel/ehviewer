<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 背景圆形 -->
    <item>
        <shape android:shape="oval">
            <solid android:color="#FF6B35" /> <!-- 橙红色背景 -->
            <size
                android:width="48dp"
                android:height="48dp" />
